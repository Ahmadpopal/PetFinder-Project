{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "examples": [
        {
            "username": "test",
            "password": "12345678"
        }
    ],
    "required": [
        "username",
        "password"
    ],
    "properties": {
        "username": {
            "type": "string",
            "examples": [
                "aaImran"
            ]
        },
        "password": {
            "type": "string",
            "examples": [
                "12345678"
            ]
        }
    },
    "additionalProperties": false
}